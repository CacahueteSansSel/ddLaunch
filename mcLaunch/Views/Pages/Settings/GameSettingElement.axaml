<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="36" Height="36"
             x:Class="mcLaunch.Views.Pages.Settings.GameSettingElement">
    <UserControl.Styles>
        <StyleInclude Source="/Styles/AppStyles.axaml"></StyleInclude>
    </UserControl.Styles>
    <DockPanel VerticalAlignment="Center">
        <TextBlock x:Name="Label" VerticalAlignment="Center" Classes="light" Margin="15 0 0 0">hello</TextBlock>
        <Panel x:Name="Contents" DockPanel.Dock="Right" Margin="0 0 5 0">
            <CheckBox x:Name="BooleanCheckbox" Checked="BooleanCheckboxChecked" Unchecked="BooleanCheckboxUnchecked"></CheckBox>
            <Slider x:Name="IntFloatSlider" PropertyChanged="IntFloatSliderPropertyChanged" Width="200" Margin="0 -13 10 0"></Slider>
        </Panel>
        <Panel></Panel>
    </DockPanel>
</UserControl>
