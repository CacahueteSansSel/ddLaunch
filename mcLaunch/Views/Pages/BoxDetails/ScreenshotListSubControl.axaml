<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="clr-namespace:mcLaunch.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="mcLaunch.Views.Pages.BoxDetails.ScreenshotListSubControl">
    <UserControl.Styles>
        <StyleInclude Source="/Styles/AppStyles.axaml"></StyleInclude>
    </UserControl.Styles>
    <ScrollViewer>
        <Panel>
            <WrapPanel x:Name="Container">
            
            </WrapPanel>
            <views:NothingToShowBanner x:Name="NtsBanner" Footer="Your in-game screenshots will show here" IsVisible="False"></views:NothingToShowBanner>
        </Panel>
    </ScrollViewer>
</UserControl>
