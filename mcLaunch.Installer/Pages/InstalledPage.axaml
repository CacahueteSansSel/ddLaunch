<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="mcLaunch.Installer.Pages.InstalledPage">
    <UserControl.Styles>
        <StyleInclude Source="/Styles/AppStyles.axaml" />
    </UserControl.Styles>

    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Vertical" Spacing="3">
            <TextBlock Classes="title" FontSize="25" Margin="0 0 0 10">mcLaunch installed successfully</TextBlock>
            <TextBlock>mcLaunch have been installed successfully on your computer, time to launch it !</TextBlock>
            <CheckBox x:Name="StartLauncherBox" IsChecked="True">Start mcLaunch</CheckBox>
        </StackPanel>
    </StackPanel>
</UserControl>